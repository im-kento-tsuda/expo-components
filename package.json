{
  "name": "@im-kento-tsuda/expo-components",
  "version": "1.0.0",
  "description": "共有Expoコンポーネントライブラリ",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/im-kento-tsuda/expo-components.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "expo": ">=50.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.73.0",
    "react-native-gesture-handler": ">=2.0.0",
    "react-native-svg": ">=13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "react": "^18.2.0",
    "react-native-gesture-handler": "^2.12.0",
    "react-native": "^0.73.0",
    "react-native-svg": "^15.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "expo",
    "react-native",
    "components",
    "ui"
  ],
  "license": "MIT",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "bash -c 'pnpm run typecheck'"
    ]
  }
}
